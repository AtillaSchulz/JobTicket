<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="Job Ticket" transaction-type="RESOURCE_LOCAL">
		<class>de.rc.jobticket.entities.Angestellte</class>
		<class>de.rc.jobticket.entities.Angestelltenbezeichnungen</class>
		<class>de.rc.jobticket.entities.Job</class>
		<class>de.rc.jobticket.entities.Jobbearbeiter</class>
		<class>de.rc.jobticket.entities.Kosten</class>
		<class>de.rc.jobticket.entities.Kunden</class>
		<class>de.rc.jobticket.entities.Produkteigenschaften</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/jobticket;create=true"/>
			<property name="javax.persistence.jdbc.user" value="user"/>
			<property name="javax.persistence.jdbc.password" value="pwd"/>
			<property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
			<property name="eclipselink.logging.level" value="FINEST"/>
		</properties>
	</persistence-unit>
</persistence>
